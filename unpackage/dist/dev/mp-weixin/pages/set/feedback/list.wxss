page {
  background-color: #f8f8f8;
}
.feedback-list {
  position: relative;
}
.feedback-list .wrapper {
  display: flex;
  flex-direction: column;
  flex: 1;
}
.feedback-list .address-box {
  display: flex;
  align-items: center;
}
.feedback-list .address-box .tag {
  font-size: 24rpx;
  margin-right: 10rpx;
  border: 1px solid;
  border-radius: 4rpx;
  padding: 4rpx 10rpx;
  line-height: 1;
}
.feedback-list .address-box .address {
  font-size: 30rpx;
  color: #303133;
}
.feedback-list .u-box {
  font-size: 28rpx;
  color: #909399;
  margin-top: 16rpx;
}
.feedback-list .u-box .name {
  margin-right: 30rpx;
}

