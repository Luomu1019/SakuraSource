.address-list {
  position: relative;
}
.address-list .address-box {
  display: flex;
  align-items: center;
}
.address-list .address-box .tag {
  font-size: 24rpx;
  margin-right: 4rpx;
  border: 1px solid;
  border-radius: 4rpx;
  padding: 0 4rpx;
}
.address-list .address-box .address {
  font-size: 30rpx;
  color: #303133;
}
.address-list .u-box {
  font-size: 28rpx;
  color: #909399;
  margin-top: 16rpx;
}
.address-list .u-box .name {
