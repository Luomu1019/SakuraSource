page {
  background-color: #f8f8f8;
}
.invoice-list {
  position: relative;
}
.invoice-list .t-box {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #303133;
}
.invoice-list .t-box .tag {
  font-size: 24rpx;
  color: #fa436a;
  margin-right: 10rpx;
  background: #fffafb;
  border: 1px solid #ffb4c7;
  border-radius: 4rpx;
  padding: 4rpx 10rpx;
  line-height: 1;
}
.invoice-list .u-box {
  font-size: 28rpx;
  color: #909399;
  margin-top: 16rpx;
}
.invoice-list .u-box .name {
  margin-right: 30rpx;
}
.wrapper {
  display: flex;
  flex-direction: column;
  flex: 1;
}
.address-box .order-fl {
  font-size: 24rpx;
  color: #909399;
  margin-bottom: 10rpx;
}
.address-box .order-fl .order-sn {
  margin-left: 14rpx;
}
.address-box .order-wrapper {
  display: flex;
  justify-content: space-between;
}
.address-box .order-wrapper .tag {
  font-size: 24rpx;
  color: #fa436a;
  margin-right: 10rpx;
  background: #fffafb;
  border: 1px solid #ffb4c7;
  border-radius: 4rpx;
  padding: 4rpx 10rpx;
  line-height: 1;
}
.address-box .order-wrapper .address {
  font-size: 30rpx;
  color: #303133;
}
.u-box {
  font-size: 28rpx;
  color: #909399;
  margin-top: 16rpx;
  display: flex;
  justify-content: space-between;
}
.u-box .time {
  font-size: 26rpx;
}

